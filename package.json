  "keywords": [
    "voice",
    "voice-actor",
    "matching",
    "platform",
    "kimitolink"
  ],
  "author": "キミトリンク工房",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "chrome-remote-interface": "^0.33.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "puppeteer": "^24.30.0",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "chrome-launcher": "^1.2.1",
    "cssnano": "^7.1.2",
    "lighthouse": "^12.8.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sharp": "^0.34.5",
    "terser": "^5.44.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
