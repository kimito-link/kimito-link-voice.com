{
  "name": "kimilink-voice",
  "version": "1.0.0",
  "description": "KimiLink Voice - 君と声でつながる声優マッチングプラットフォーム",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node build-production.js",
    "build:css": "postcss css/styles.css -o dist/css/styles.min.css",
    "convert:webp": "node convert-images-to-webp.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:performance": "node test-performance.js",
    "lighthouse": "node lighthouse-auto.js",
    "lighthouse:history": "node lighthouse-history-viewer.js",
    "lighthouse:auto": "node lighthouse-auto-commit.js",
    "debug": "node auto-debug.js",
    "fix": "node debug-and-fix.js",
    "birthday": "node birthday-shizuku/birthday-automation.js"
